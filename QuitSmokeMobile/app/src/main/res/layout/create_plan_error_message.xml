<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_create_plan_empty_target"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/errorBackgound"/>

        <TextView
            android:id="@+id/tv_create_plan_no_partner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/errorBackgound"/>

        <TextView
            android:id="@+id/tv_create_plan_proceeding_plan_exist"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/errorBackgound"/>

        <TextView
            android:id="@+id/tv_not_create_plan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/errorBackgound"/>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>